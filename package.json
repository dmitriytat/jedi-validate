    "lint": "eslint src/**/*.js",
    "fix": "eslint src/**/*.js --fix",
    "fix-tests": "eslint tests/**/*.js --fix",
    "esdoc": "esdoc -c esdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmitriytat/jedi-validate.git"
  },
  "keywords": [
